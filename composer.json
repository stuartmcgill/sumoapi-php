{
    "name": "stuartmcgill/sumoapi-php",
    "description": "A PHP wrapper for https://sumo-api.com/",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "StuartMcGill\\SumoApiPhp\\": "src/"
        }
    },
    "autoload-dev" :
    {
        "psr-4" :
        {
            "StuartMcGill\\SumoApiPhp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stuart McGill"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan-mockery": "^1.1"
    },
    "require": {
        "php": ">=8.2",
        "guzzlehttp/guzzle": "^7.5"
    },
    "scripts": {
        "tests": "vendor/bin/phpunit tests",
        "phpstan": "vendor/bin/phpstan analyse"
    }
}
